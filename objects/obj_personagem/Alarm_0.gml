/// @description Timer Geral
alarm[0] = 100

if (ultimate < status.ulttotal) && ultativo = false{
if (ultimate+status.recargault) <= status.ulttotal{
ultimate += status.recargault}
else{
ultimate = status.ulttotal
}
}

if status.flechas < global.localups.bowammo.status{
status.flechas++
}

if status.bombas < global.localups.bombammo.status{
status.bombas ++
}